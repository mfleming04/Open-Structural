  </ItemGroup>
  <ItemGroup>
    <Compile Include="Class1.cs" />
    <Compile Include="Properties\AssemblyInfo.cs" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="OpenStructuralImags.resx" />
  </ItemGroup>
  <ItemGroup>
    <None Include="Resources\DefaultCustomNode.Large.png" />
